local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = lib.CreateLib("pain", "Sentinel")
local Tab = Window:NewTab("main")
local Section = Tab:NewSection("rip this game")
local Tabclient = Window:NewTab("client")
local Client = Tabclient:NewSection("chill")


-- main

Section:NewLabel("made by ! EMPLIC#9999")
Section:NewLabel("discord.gg/jCsStxTJMc")


Section:NewButton("get all gamepasses", "lel", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/offperms/gamepass/main/forallgames"))()
end)

Section:NewToggle("run wind (wind gun only)", "runs wind automatically", function(state)
    if state then
        -- Script generated by R2Sv2
-- R2Sv2 developed by Luckyxero
 
local A_1 = "Run Wind"
local Event = game:GetService("Workspace").Emplic["Wind Gun"].RemoteEvent
Event:FireServer(A_1)

    else
        -- Script generated by R2Sv2
-- R2Sv2 developed by Luckyxero
 
local A_1 = "Stop Wind"
local Event = game:GetService("Workspace").Emplic["Wind Gun"].RemoteEvent
Event:FireServer(A_1)

    end
end)


Section:NewButton("get all paid weapons", "lel", function()
    
local A_1 = "Give Weapon"
local A_2 = "Wind Gun"
local Event = game:GetService("ReplicatedStorage")["Weapon_Event"]
Event:FireServer(A_1, A_2)

 
local A_1 = "Give Weapon"
local A_2 = "Boom Bat"
local Event = game:GetService("ReplicatedStorage")["Weapon_Event"]
Event:FireServer(A_1, A_2)

end)

Section:NewToggle("auto unragdoll", "e", function(v)
  getgenv().unragdoll = v
  while true do
      if not getgenv().unragdoll then return end
      
     
      
      
      -- Script generated by R2Sv2
-- R2Sv2 developed by Luckyxero
 
-- Script generated by R2Sv2
-- R2Sv2 developed by Luckyxero
 
local A_1 = "Unragdoll"
local Event = game:GetService("ReplicatedStorage")["Click_Event"]
Event:FireServer(A_1)

wait(0.1)
end

end)




Client:NewButton("hitbox expander", "lel", function()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/offperms/hitbox/main/expander"))()
end)

Client:NewButton("Naked","No clothes for you",function()
        for i,v in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
    		if v:IsA("Clothing") or v:IsA("ShirtGraphic") then
    			v:Destroy()
    		end
    	end
end)

Client:NewButton("Run Flying Script (E)", "Press (E) to use.", function() if _G.flyscriptenabled then return; end; _G.flyscriptenabled = true; return loadstring(game:HttpGet("https://raw.githubusercontent.com/offperms/fly/main/lmao"))(); end)
